{
  "name": "RPNet Link Generator",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Link generator for RPNet.biz",
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://api.flickr.com/", "https://premium.rpnet.biz/", "contextMenus"
  ]
}